<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.butfly.albacore.demo.mapper.User">
	<select id="load" resultType="net.butfly.albacore.demo.mapper.User" parameterType="java.lang.String">
        select id, name from users where id=#{value}
	</select>
	<select id="count" resultType="int">
        select count(*) from users
	</select>
	<select id="select" resultType="net.butfly.albacore.demo.mapper.User">
        select id, name from users
	</select>
</mapper>
