<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties resource="mybatis.properties" />
	<settings>
		<setting name="cacheEnabled" value="${ibatis.cacheEnabled}" />
		<setting name="logImpl" value="SLF4J" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<plugins>
		<plugin interceptor="net.butfly.albacore.dbo.interceptor.StatementInterceptor">
			<property name="dialect" value="net.butfly.albacore.dbo.dialect.OracleDialect" />
		</plugin>
		<plugin interceptor="net.butfly.albacore.dbo.interceptor.ExecutorInterceptor">
			<property name="timestampProps" value="addTime" />
			<property name="keyGenerator" value="net.butfly.albacore.dbo.key.ObjectIdKeyGenerator" />
		</plugin>
	</plugins>
	<mappers>
		<mapper resource="mybatis-cache.xml" />

		<mapper resource="net/butfly/albacore/demo/mapper/User.xml" />
	</mappers>
</configuration>  
