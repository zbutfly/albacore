<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">	<import resource="classpath:/net/butfly/albacore/orm/spring/beans-dao.xml" />
	<import resource="classpath:/net/butfly/albacore/orm/spring/beans-tx.xml" />	<bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">		<property name="order" value="999" />		<property name="ignoreResourceNotFound" value="true" />		<property name="ignoreUnresolvablePlaceholders" value="false" />		<property name="location" value="classpath:mybatis.properties" />		<property name="properties">			<props>				<!-- for mybatis bean definition -->				<prop key="ibatis.template">BATCH</prop>				<prop key="ibatis.template.batch">BATCH</prop>				<prop key="ibatis.config.location.pattern">classpath*:**/mybatis-config.xml</prop>				<prop key="ibatis.cache.config.location.pattern">classpath*:**/mybatis-cache.xml</prop>				<!-- for mybatis bean internal definition -->				<prop key="ibatis.cacheEnabled">false</prop>				<prop key="ibatis.defaultStatementTimeout">120</prop>				<prop key="ibatis.defaultExecutorType">BATCH</prop>				<prop key="ibatis.lazyLoadingEnabled">false</prop>				<prop key="ibatis.aggressiveLazyLoading">false</prop>			</props>		</property>	</bean></beans>
